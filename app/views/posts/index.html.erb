<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <%= paginate @posts %>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.body %>Written by: <%= post.user.username %></td>
        </tr>
      <% end %>
  </tbody>
</table>

<br>


<%= link_to 'New Post', new_post_path %>
<%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>
